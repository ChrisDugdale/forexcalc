        <TextBox Height="23" HorizontalAlignment="Left" Margin="84,239,0,0" Name="buyPriceText" VerticalAlignment="Top" Width="118" TextChanged="buyPriceText_TextChanged" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="417,238,0,0" Name="sellPriceText" VerticalAlignment="Top" Width="118" />
