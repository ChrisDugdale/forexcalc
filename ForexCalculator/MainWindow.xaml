        <Label Margin="451,164,0,0" Name="sellOrderLabel" Height="28" VerticalAlignment="Top" HorizontalAlignment="Left" Width="84" />
        <Label Content="Insert take profit level" Height="28" HorizontalAlignment="Left" Margin="208,106,0,0" Name="MiddleTakeProfitLabel" VerticalAlignment="Top" Width="203" HorizontalContentAlignment="Center" FontSize="14" />
        <Label Content="Insert Stop Loss level" Height="28" HorizontalAlignment="Left" Margin="208,135,0,0" Name="middleStopLossLabel" VerticalAlignment="Top" Width="203" HorizontalContentAlignment="Center" FontSize="14" />
        <Label Content="Insert Order Level" Height="28" HorizontalAlignment="Left" Margin="208,164,0,0" Name="middleOrderLevel" VerticalAlignment="Top" Width="203" HorizontalContentAlignment="Center" FontSize="14" />
